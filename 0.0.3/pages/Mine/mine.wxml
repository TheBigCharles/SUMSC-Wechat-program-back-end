<!--pages/Mine/mine.wxml-->
<image src="../Welcome/windows1.png" class="img0"></image>
<image src="../Welcome/windows2.png" class="img00"></image>

<!--顶部自制导航栏-->
<view class="top0">
  <view class="top1">
    <image class="topicon" src="../../image/icon/global2.png" bindtap='onSelect'></image>
    <text class="topword">Select Activities</text>
  </view>
  <view class="top1">
    <image class="topicon" src="../../image/icon/signin2.png" bindtap='onSignin'></image>
    <text class="topword">Sign in</text>
  </view>
  <view class="top1">
    <image class="topicon" src="../../image/icon/path2.png" bindtap='onNavigation'></image>
    <text class="topword">Navigation</text>
  </view>
</view>

<!--向服务器请求当前用户的信息-->
<view class='error0'>
  <image class="topicon" src="../../image/icon/error.png" bindtap='onSelect'></image>
  <text class='error'>暂未接入校园认证数据库</text>
</view>